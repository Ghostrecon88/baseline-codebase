{
  "name": "rex-query",
  "version": "0.3.0",
  "private": true,
  "main": "./src/index.js",
  "rex": {},
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/react-scripts/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/node_modules/react-scripts/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/react-scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "snapshotSerializers": [
      "<rootDir>/scripts/jest-serialize-context.js",
      "<rootDir>/scripts/jest-serialize-domain.js"
    ]
  },
  "devDependencies": {
    "babel-jest": "^17.0.0",
    "flow-bin": "0.37.4",
    "flow-coverage-report": "^0.2.0",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "pegjs": "^0.10.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-scripts": "^0.8.4"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "dependencies": {
    "@prometheusresearch/react-ui": "^0.22.3",
    "@prometheusresearch/react-datetimepicker": "^0.5.0",
    "babel-eslint": "7.0.0",
    "debounce-promise": "^2.1.1",
    "downloadjs": "^1.4.6",
    "eslint": "3.8.1",
    "eslint-config-react-app": "0.5.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "invariant": "^2.2.1",
    "lodash": "^4.15.0",
    "moment": "^2.15.2",
    "qs": "^6.2.1",
    "react-addons-shallow-compare": "^0.14.0",
    "react-aria-menubutton": "andreypopp/react-aria-menubutton#939562363c5fb9eecdc38465477e3c209339383c",
    "react-icons": "^2.2.1",
    "react-relative-portal": "^1.2.0",
    "react-select": "^1.0.0-rc.2",
    "react-stylesheet": "^2.0.0",
    "react-virtualized": "^8.7.1"
  },
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
