{
    "instrument": {
        "id": "urn:test-instrument",
        "version": "1.1"
    },
    "defaultLocalization": "en",
    "title": {
        "en": "Form with calculated fields"
    },
    "pages": [
        {
            "id": "page1",
            "elements": [
                {
                    "type": "text",
                    "options": {
                        "text": {
                            "en": "Note that this form has two fields with calculated values. But only one of those is rendered."
                        }
                    }
                },
                {
                    "type": "question",
                    "options": {
                        "fieldId": "q_name",
                        "text": {
                            "en": "What's your name?"
                        }
                    }
                },
                {
                    "type": "question",
                    "options": {
                        "fieldId": "q_age",
                        "text": {
                            "en": "How old are you?"
                        },
                        "help": {
                            "en": "If you are more than 18 years old you should answer for more questions!"
                        },
                        "events": [
                            {
                                "action": "calculate",
                                "trigger": "if(q_age<18,'Moscow',null())",
                                "targets": ["q_city"]
                            },
                            {
                                "action": "calculate",
                                "trigger": "if(q_age<18,'Russia',null())",
                                "targets": ["q_country"]
                            }
                        ]
                    }
                },
                {
                    "type": "question",
                    "options": {
                        "fieldId": "q_country",
                        "text": {
                            "en": "Country?"
                        }
                    }
                },
                {
                    "type": "question",
                    "options": {
                        "fieldId": "q_place",
                        "text": {
                            "en": "Place?"
                        }
                    }
                }
            ]
        }
    ]
}

